{
  "name": "ConTask",
  "version": "0.1.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest --watch",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "electron": "set NODE_ENV=dev&& tsc && concurrently \"npm run tsc:w\" \"electron .\"",
    "bundle.windows": "electron-packager . --platform=win32 --overwrite --out=bundles",
    "bundle.linux": "electron-packager . --platform=linux --overwrite --out=bundles",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0-rc.6",
    "@angular/cdk": "^6.0.0-rc.14",
    "@angular/common": "^6.0.0-rc.6",
    "@angular/compiler": "^6.0.0-rc.6",
    "@angular/core": "^6.0.0-rc.6",
    "@angular/forms": "^6.0.0-rc.6",
    "@angular/http": "^6.0.0-rc.6",
    "@angular/material": "^6.0.0-rc.14",
    "@angular/platform-browser": "^6.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.6",
    "@angular/router": "^6.0.0-rc.6",
    "@ngrx/effects": "^6.0.0-beta.1",
    "@ngrx/entity": "^6.0.0-beta.1",
    "@ngrx/store": "^6.0.0-beta.1",
    "@ngrx/store-devtools": "^6.0.0-beta.1",
    "core-js": "^2.5.5",
    "rxjs": "^6.0.0-uncanny-rc.7",
    "ngrx-store-localstorage": "^5.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.5.11",
    "@angular/cli": "^6.0.0-rc.8",
    "@angular/compiler-cli": "^6.0.0-rc.6",
    "@angular/language-service": "^6.0.0-rc.6",
    "@ngrx/schematics": "^5.2.0",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^22.2.3",
    "@types/node": "^6.0.107",
    "angular-testing-library": "^0.0.2",
    "codelyzer": "^4.3.0",
    "electron": "^1.8.6",
    "electron-log": "^2.2.14",
    "electron-packager": "^11.2.0",
    "electron-updater": "^2.21.8",
    "hammerjs": "^2.0.8",
    "jasmine-core": "^3.1.0",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^22.4.3",
    "jest-preset-angular": "^5.2.2",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "protractor": "^5.3.1",
    "tslint": "^5.9.1",
    "typescript": "~2.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "moduleNameMapper": {
      "^@app/state(.*)$": "<rootDir>/src/app/state/$1",
      "^@app/(.*)$": "<rootDir>/src/app/$1"
    }
  }
}
